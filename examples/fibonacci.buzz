import "std";

fun fibonacci(n: int) > void *> int? {
    var n1 = 0;
    var n2 = 1;
    var next: int? = null;

    foreach (_ in 0..n) {
        _ = yield n1;
        next = n1 + n2;
        n1 = n2;
        n2 = next!;
    }
}

fun main(args: [str]) > void {
    const N = std\parseInt(args[?0] ?? "10") ?? 10;

    foreach (n in &fibonacci(N)) {
        std\print("{n}");
    }
}
